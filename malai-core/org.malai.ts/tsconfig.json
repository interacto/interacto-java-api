{
  "compilerOptions": {
    "target": "esnext",
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "module": "ESnext",
    "outDir": "target",
    "pretty": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "noImplicitReturns": true,
    "declaration": true,
    "types": [
      "jest"
    ]
  }
}